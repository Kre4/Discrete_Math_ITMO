# n task
from sage.graphs.connectivity import spqr_tree
H=Graph({"AL":["GR","ME","MK","RS"],"GR":["BG","MK","TR"],"ME":["BA","HR","KO","RS"],"MK":["BG","RS"],"RS":["BA","BG","HR","HU","KO","RO"],"AT":["CZ","DE","HU","IT","LI","SK","SL","CH"],"CZ":["DE","PL","SK"],"DE":["BE","FR","LU","NL","PL","CH"],"HU":["HR","SK","SL","UA"],"IT":["FR","SL","CH"],"LI":["CH"],"SK":["PL","UA"],"SL":["HR"],"CH":["FR"],"BY":["LV","LT","PL","RU","UA"],"LV":["EE","LT","RU"],"LT":["PL","RU"],"PL":["RU","UA"],"RU":["EE","UA"],"UA":["MD","RO"],"BE":["FR","LU","NL"],"FR":["LU"],"BA":["HR"],"BG":["RO","TR"],"RO":["MD"]}
)
H = H.to_undirected()
tree = spqr_tree(H)

plot(tree)

